    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently -n api,queue,schedule,vite,stripe -c auto \"php artisan serve\" \"php artisan queue:work --tries=3\" \"php artisan schedule:work\" \"vite\" \"npm:stripe\"",
